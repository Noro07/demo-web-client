  "description": "This is a demo for developing an web client for all skills",
  "scripts": {
    "build": "lerna run build --scope web-server --stream",
    "check:lint": "lerna run check:lint",
    "clean": "lerna clean --yes && rimraf ./node_modules",
    "prelink-packages": "npm install",
    "link-packages": "lerna bootstrap",
    "start": "lerna run start --scope web-server --stream",
    "start:dev": "lerna run start:dev --scope web-server --stream",
    "test": "npm run check:lint && npm run test:unit",
    "test:unit": "lerna exec npm run test:unit --scope \"@(landing-web)\" --stream --concurrency 1",
    "pretest:e2e": "webdriver-manager update --ignore_ssl",
    "test:e2e": "protractor protractor.conf.js"
  },
  "repository": {
